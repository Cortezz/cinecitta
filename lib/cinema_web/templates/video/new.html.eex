<%= form_for @changeset, video_path(@conn, :create), [multipart: true], fn f -> %>
    <div class="form-group">
        <label>Video</label>
        <%= file_input f, :video, class: "form-control" %>
    </div>

    <div class="form-group">
        <%= submit "Submit", class: "btn btn-primary" %>
    </div>
<% end %>